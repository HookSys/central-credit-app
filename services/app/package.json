{
    "name": "@central-credit/app",
    "version": "0.0.3",
    "private": true,
    "description": "Application Central Credit",
    "keywords": [
        "jcf"
    ],
    "author": "Daniel Franco <dafrannco@gmail.com>",
    "homepage": "",
    "license": "ISC",
    "main": "index.js",
    "directories": {
        "dist": "dist",
        "test": "__tests__"
    },
    "files": [
        "index.js",
        "index.d.ts",
        "dist",
        "src",
        "!*/__tests__"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://www.npmjs.com/package/@central-credit/app"
    },
    "nodemonConfig": {
        "verbose": true,
        "watch": [
            "src/"
        ],
        "ignore": [
            "dist/*"
        ],
        "ext": "ts",
        "exec": "concurrently --kill-others \"npm run start\""
    },
    "scripts": {
        "build": "lb-tsc",
        "prestart": "npm run build",
        "start:watch": "nodemon",
        "start": "node --inspect=0.0.0.0:9229 -r source-map-support/register .",
        "clean": "lb-clean dist *.tsbuildinfo",
        "docker:build": "docker build -t jcf-app:$npm_package_version .",
        "docker:run": "docker run -p 3000:3000 -d jcf-app:$npm_package_version"
    },
    "dependencies": {
        "@loopback/authentication": "^3.3.2",
        "@loopback/boot": "^1.7.3",
        "@loopback/context": "^2.1.0",
        "@loopback/core": "^1.12.3",
        "@loopback/openapi-v3": "^1.13.0",
        "@loopback/repository": "^1.19.0",
        "@loopback/rest": "^1.26.1",
        "@loopback/rest-explorer": "^1.4.9",
        "@loopback/service-proxy": "^1.3.16",
        "concurrently": "^5.1.0",
        "jsonwebtoken": "^8.5.1",
        "loopback-connector-mongodb": "^5.2.1",
        "loopback-connector-rest": "^3.6.0"
    },
    "devDependencies": {
        "@types/source-map-support": "^0.5.1",
        "debug": "^4.1.1",
        "source-map-support": "^0.5.16"
    }
}
