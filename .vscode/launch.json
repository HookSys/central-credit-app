{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run/Debug on Kubernetes",
      "type": "cloudcode.kubernetes",
      "request": "launch",
      "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
      "watch": true,
      "cleanUp": true,
      "portForward": true,
      "imageRegistry": "gcr.io/central-credit-cloud"
    },
    {
      "name": "Local Process with Kubernetes (Preview)",
      "appFolder": "${workspaceFolder}/services/app",
      "type": "dev-spaces-connect-configuration",
      "request": "launch"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}"
    }
  ]
}