{
  "name": "@central-credit/app",
  "version": "0.0.3",
  "private": true,
  "description": "Application Central Credit",
  "keywords": [
    "jcf"
  ],
  "author": "Daniel Franco <dafrannco@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "index.js",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://www.npmjs.com/package/@central-credit/app"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "concurrently --kill-others \"npm run start\""
  },
  "scripts": {
    "build": "lb-tsc",
    "prestart": "npm run build",
    "start:watch": "nodemon",
    "start": "node --inspect=0.0.0.0:9229 -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo",
    "docker:build": "docker build -t jcf-app:$npm_package_version .",
    "docker:run": "docker run -p 3000:3000 -d jcf-app:$npm_package_version"
  },
  "dependencies": {
    "@loopback/authentication": "^3.3.1",
    "@loopback/boot": "^1.7.2",
    "@loopback/context": "^2.0.0",
    "@loopback/core": "^1.12.2",
    "@loopback/openapi-v3": "^1.12.0",
    "@loopback/repository": "^1.18.0",
    "@loopback/rest": "^1.26.0",
    "@loopback/rest-explorer": "^1.4.8",
    "@loopback/service-proxy": "^1.3.15",
    "debug": "^4.1.1",
    "loopback-connector-mongodb": "^5.2.1",
    "loopback-connector-rest": "^3.6.0",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.0",
    "@loopback/eslint-config": "^5.0.2",
    "@loopback/testlab": "^1.10.2",
    "@types/node": "^10.17.13",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.5"
  }
}
